{
	"name": "DF_USUARIO",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_ADLS_PARQUET",
						"type": "DatasetReference"
					},
					"name": "SourceUsuario"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_ADLS_PARQUET",
						"type": "DatasetReference"
					},
					"name": "SinkHubUsuario"
				},
				{
					"dataset": {
						"referenceName": "DS_ADLS_PARQUET",
						"type": "DatasetReference"
					},
					"name": "SinkSatUsuario"
				}
			],
			"transformations": [
				{
					"name": "DerivedColumn"
				},
				{
					"name": "HubUsuarioSelect"
				},
				{
					"name": "SatUsuario"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Id as string,",
				"          CreationDate as string,",
				"          DownVotes as string,",
				"          LastAccessDate as string,",
				"          Reputation as string,",
				"          UpVotes as string,",
				"          Views as string,",
				"          AccountId as string,",
				"          Email as string,",
				"          location as string,",
				"          update as string,",
				"          owner as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> SourceUsuario",
				"SourceUsuario derive(HK_USUARIO = Id,",
				"          CODUSUARIO = AccountId,",
				"          FECCARGA = currentUTC(),",
				"          REGFUENTE = \"SHAREDKNOW\",",
				"          FECCREATION = CreationDate,",
				"          NUMDOWNVOTES = DownVotes,",
				"          EMAIL = Email) ~> DerivedColumn",
				"DerivedColumn select(mapColumn(",
				"          HK_USUARIO,",
				"          CODUSUARIO,",
				"          FECCARGA,",
				"          REGFUENTE",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> HubUsuarioSelect",
				"DerivedColumn select(mapColumn(",
				"          HK_USUARIO,",
				"          FECCARGA,",
				"          REGFUENTE,",
				"          FECCREATION,",
				"          NUMDOWNVOTES,",
				"          EMAIL = DerivedColumn@EMAIL",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SatUsuario",
				"HubUsuarioSelect sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SinkHubUsuario",
				"SatUsuario sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> SinkSatUsuario"
			]
		}
	}
}