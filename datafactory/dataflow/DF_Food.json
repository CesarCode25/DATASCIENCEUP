{
	"name": "DF_Food",
	"properties": {
		"folder": {
			"name": "TrabajoFinal"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DFood_Parquet",
						"type": "DatasetReference"
					},
					"name": "FuenteFood"
				},
				{
					"dataset": {
						"referenceName": "DMenu_Parquet",
						"type": "DatasetReference"
					},
					"name": "FuenteMenu"
				},
				{
					"dataset": {
						"referenceName": "DOrdenes_Parquet",
						"type": "DatasetReference"
					},
					"name": "FuenteOrdenes"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DFood_Parquet",
						"type": "DatasetReference"
					},
					"name": "DestinoFood"
				},
				{
					"dataset": {
						"referenceName": "DMenu_Parquet",
						"type": "DatasetReference"
					},
					"name": "DestinoMenu"
				}
			],
			"transformations": [
				{
					"name": "DerivadaFood"
				},
				{
					"name": "CuatroColumnas"
				},
				{
					"name": "DerivadaFuenteMenu"
				},
				{
					"name": "select1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          N as string,",
				"          f_id as string,",
				"          item as string,",
				"          veg_or_non_veg as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> FuenteFood",
				"source(output(",
				"          N as string,",
				"          f_id as string,",
				"          item as string,",
				"          veg_or_non_veg as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> FuenteMenu",
				"source(output(",
				"          N as string,",
				"          f_id as string,",
				"          item as string,",
				"          veg_or_non_veg as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> FuenteOrdenes",
				"FuenteFood derive(FechaActualizacion = currentUTC()) ~> DerivadaFood",
				"DerivadaFood select(mapColumn(",
				"          f_id,",
				"          item,",
				"          veg_or_non_veg,",
				"          FechaActualizacion",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> CuatroColumnas",
				"FuenteMenu derive(FechadeActualizacion = currentUTC()) ~> DerivadaFuenteMenu",
				"DerivadaFuenteMenu select(mapColumn(",
				"          f_id,",
				"          item,",
				"          veg_or_non_veg,",
				"          FechadeActualizacion",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"FuenteOrdenes derive() ~> derivedColumn1",
				"CuatroColumnas sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> DestinoFood",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          N as string,",
				"          f_id as string,",
				"          item as string,",
				"          veg_or_non_veg as string",
				"     ),",
				"     format: 'parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> DestinoMenu"
			]
		}
	}
}